{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "typeRoots": [
      "./node_modules/@types",
      "./src/types" // 여기에 추가
    ]
    },
    "include": ["src/**/*","src/types/**/*"],
    "exclude": ["node_modules"]
  }
  